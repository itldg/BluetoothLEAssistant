                        <!--<Button
                            Grid.Row="1"
                            Grid.Column="1"
                            Command="ScrollBar.ScrollHereCommand" />-->
                    </Grid>

                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>