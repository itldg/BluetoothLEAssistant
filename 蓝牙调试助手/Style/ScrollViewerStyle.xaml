                            <DoubleAnimation
                                BeginTime="0:0:0.3"
                                Storyboard.TargetName="PART_Track"
                                Storyboard.TargetProperty="(FrameworkElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleX)"
                                To="1"
                                Duration="00:00:00.15" />

                            <DoubleAnimation
                                BeginTime="0:0:0.3"
                                Storyboard.TargetName="BgInner"
                                Storyboard.TargetProperty="Opacity"
                                To="1"
                                Duration="00:00:00.15" />
                            <DoubleAnimation
                                BeginTime="0:0:0.3"
                                Storyboard.TargetName="VerticalSmallDecrease"
                                Storyboard.TargetProperty="Opacity"
                                To="1"
                                Duration="00:00:00.15" />
                            <DoubleAnimation
                                BeginTime="0:0:0.3"
                                Storyboard.TargetName="VerticalSmallIncrease"
                                Storyboard.TargetProperty="Opacity"
                                To="1"
                                Duration="00:00:00.15" />
                        </Storyboard>
                    </BeginStoryboard>
                </Trigger.EnterActions>

                <Trigger.ExitActions>
                    <BeginStoryboard>
                        <Storyboard>
                            <DoubleAnimation
                                BeginTime="0:0:0.6"
                                Storyboard.TargetName="PART_Track"
                                Storyboard.TargetProperty="(FrameworkElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleX)"
                                To="0.5"
                                Duration="00:00:00.15" />

                            <DoubleAnimation
                                BeginTime="0:0:0.6"
                                Storyboard.TargetName="BgInner"
                                Storyboard.TargetProperty="Opacity"
                                To="0"
                                Duration="00:00:00.15" />

                            <DoubleAnimation
                                BeginTime="0:0:0.6"
                                Storyboard.TargetName="VerticalSmallDecrease"
                                Storyboard.TargetProperty="Opacity"
                                To="0"
                                Duration="00:00:00.15" />
                            <DoubleAnimation
                                BeginTime="0:0:0.6"
                                Storyboard.TargetName="VerticalSmallIncrease"
                                Storyboard.TargetProperty="Opacity"
                                To="0"
                                Duration="00:00:00.15" />
                        </Storyboard>
                    </BeginStoryboard>

                    <StopStoryboard BeginStoryboardName="ismouse" />
                </Trigger.ExitActions>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>



    <!--  ScrollBar样式  -->
    <Style x:Key="DefaultScrollBar" TargetType="{x:Type ScrollBar}">
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="OverridesDefaultStyle" Value="true" />
        <Style.Triggers>
            <Trigger Property="Orientation" Value="Horizontal">
                <Setter Property="Template" Value="{StaticResource HorizontalScrollBar}" />
            </Trigger>
            <Trigger Property="Orientation" Value="Vertical">
                <Setter Property="Template" Value="{StaticResource VerticalScrollBar}" />
            </Trigger>
        </Style.Triggers>
    </Style>


    <!--  ScrollViewer样式  -->
    <Style x:Key="CustomScrollViewerStyle" TargetType="{x:Type ScrollViewer}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ScrollViewer}">
                    <Grid x:Name="Grid" Background="{TemplateBinding Background}">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="AUTO" />
                        </Grid.ColumnDefinitions>

                        <ScrollContentPresenter
                            x:Name="PART_ScrollContentPresenter"
                            Margin="{TemplateBinding Padding}"
                            CanContentScroll="{TemplateBinding CanContentScroll}"
                            CanHorizontallyScroll="False"
                            CanVerticallyScroll="False"
                            Content="{TemplateBinding Content}"
                            ContentTemplate="{TemplateBinding ContentTemplate}" />
                        <!--  垂直滚动条  -->
                        <ScrollBar
                            x:Name="PART_VerticalScrollBar"
                            Grid.Column="1"
                            Margin="0,0,2,12"
                            HorizontalAlignment="Right"
                            AutomationProperties.AutomationId="VerticalScrollBar"
                            Cursor="Arrow"
                            Maximum="{TemplateBinding ScrollableHeight}"
                            Minimum="0"
                            Style="{StaticResource DefaultScrollBar}"
                            ViewportSize="{TemplateBinding ViewportHeight}"
                            Visibility="{TemplateBinding ComputedVerticalScrollBarVisibility}"
                            Value="{Binding VerticalOffset, Mode=OneWay, RelativeSource={RelativeSource TemplatedParent}}" />
                        <!--  水平底部滚动条  -->
                        <ScrollBar
                            x:Name="PART_HorizontalScrollBar"
                            Margin="0,0,12,2"
                            VerticalAlignment="Bottom"
                            AutomationProperties.AutomationId="HorizontalScrollBar"
                            Cursor="Arrow"
                            Maximum="{TemplateBinding ScrollableWidth}"
                            Minimum="0"
                            Orientation="Horizontal"
                            Style="{StaticResource DefaultScrollBar}"
                            ViewportSize="{TemplateBinding ViewportWidth}"
                            Visibility="{TemplateBinding ComputedHorizontalScrollBarVisibility}"
                            Value="{Binding HorizontalOffset, Mode=OneWay, RelativeSource={RelativeSource TemplatedParent}}" />

                        <!--<Button
                            Grid.Row="1"
                            Grid.Column="1"
                            Command="ScrollBar.ScrollHereCommand" />-->
                    </Grid>

                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!--  ScrollViewer样式 滚动条与内容处于统一容器  -->
    <Style x:Key="CustomScrollViewerStyle1" TargetType="{x:Type ScrollViewer}">
        <Setter Property="Template">
            <Setter.Value>
